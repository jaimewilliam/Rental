@model IEnumerable<SimpleRental.Models.Genre>

<div style="position:relative; width:100%; height:100%">
    <span class="moviegenrefk" style="display:none">@ViewBag.GenreId</span>
    <select class="SelectGenre">
        @foreach (var item in Model)
        {
            <option disabled value="@item.GenreID">@item.GenreName</option>
        }
    </select>
</div>

@Scripts.Render("~/bundles/jquery")